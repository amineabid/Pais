<div class="form-wraper">
    <div class="form-container card">
        <div class="form-header">
            <img src="data:image/svg+xml;base64," class="main-lock-icon" alt="">
        </div>

        <div class="form-body">
            <mat-form-field appearance="outline"class="justifier">
                <input matInput type="text"  placeholder="Email"  >
            </mat-form-field>
            <mat-form-field appearance="outline" class="justifier">
                <input matInput  [type]="hide ? 'password' : 'text'" placeholder="Password"  >
                <button mat-icon-button matSuffix (click)="hide = !hide" [attr.aria-label]="'Hide password'" [attr.aria-pressed]="hide">
                    <mat-icon>{{hide ? 'visibility_off' : 'visibility'}}</mat-icon>
                </button>
            </mat-form-field>
        </div>

        <div class="form-footer">
            <button mat-raised-button [routerLink]="['/admin']" routerLinkActive="router-link-active"
                class="btn custom-btn">
                <mat-icon>input</mat-icon> Login
            </button>
        </div>
        <!-- <div class="additional-link">
            <a routerLink="/signup">No Account? <strong>Create one here</strong></a>
        </div> -->
    </div>
</div>